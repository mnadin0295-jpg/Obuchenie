ids = {
    'user1': [213, 213, 213, 15, 213],
    'user2': [54, 54, 119, 119, 119],
    'user3': [213, 98, 98, 35]
}

# объединяем все списки значений в один
all_geo_tags = []
for tags in ids.values():
    all_geo_tags.extend(tags)

# преобразуем в множество для получения уникальных значений
unique_geo_tags = set(all_geo_tags)

print(unique_geo_tags)
